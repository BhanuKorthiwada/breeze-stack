{
  "name": "breezestack",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "db:d1:generate": "dotenv -- drizzle-kit generate --config=drizzle.cloudflare_d1.config.ts",
    "db:d1:migrate": "wrangler d1 migrations apply --local DB",
    "db:d1:migrate:development": "wrangler d1 migrations apply DB --env development --remote",
    "db:d1:migrate:staging": "wrangler d1 migrations apply DB --env staging --remote",
    "db:d1:migrate:production": "wrangler d1 migrations apply DB --env production --remote",
    "db:d1:migrate-production": "dotenv -- drizzle-kit migrate --config=drizzle.cloudflare_d1.config.ts",
    "deploy:wrangler:development": "wrangler deploy --env development",
    "deploy:wrangler:staging": "wrangler deploy --env staging",
    "deploy:wrangler:production": "wrangler deploy --env production",
    "deploy:development": "run-s build deploy:wrangler:development",
    "deploy:staging": "run-s build deploy:wrangler:staging",
    "deploy:production": "run-s build deploy:wrangler:production",
    "dev": "react-router dev",
    "start": "wrangler dev",
    "typecheck": "react-router typegen && tsc -b",
    "generate:types": "wrangler types --env-interface CloudflareWorkerEnvironment"
  },
  "dependencies": {
    "@conform-to/react": "^1.2.2",
    "@conform-to/zod": "^1.2.2",
    "@fontsource-variable/inter": "^5.2.5",
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
